<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
    <div class="container-fluid p-0 pt-3">
      <div class="row">
        <div class="<%= typeof showSelect != 'undefined' ? 'col-md-7' : '' %> col-12">
          <% if(typeof showSelect != 'undefined') { %>
          <div class="container-fluid my-3">
            <div class="row">
              <div class="col-6">
                <select id="tipologia-select" class="form-select form-select-sm" data-filter="tipologia" aria-label=".form-select-sm example">
                  <option selected value='base'>Seleziona la tipologia</option>
                  <option value="uscita">Uscita</option>
                  <option value="entrata">Entrata</option>
                </select>
              </div>
              <div class="col-6">
                <select id="categoria-select" class="form-select form-select-sm" data-filter="categoria" aria-label=".form-select-sm example">
                  <option selected>Seleziona la categoria</option>
                </select>
              </div>
            </div>
          </div>
          <% } %>
          <% if(typeof changes != 'undefined' && changes) { %>
            <div class="alert alert-warning" role="alert">
              L'applicazione Ã¨ stata aggiornata! Riavviala per rendere effettive le modifiche!
            </div>
          <% } %>
          <div class="table-responsive">
              <table id="detail" class="table table-striped table-sm"></table>
          </div>
        </div>
        <div class="<%= typeof showSelect != 'undefined' ? 'col-md-5' : '' %> col-12">
          <div class="container-fluid p-0">
            <div class="row">
              <div id="income_container" class="<%= typeof showSelect == 'undefined' ? 'col-md-6' : '' %> col-12">
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                  <h1 class="h5">Grafico entrate <span class="total"></span></h1>
                </div>
                <canvas class="my-4 w-100" id="entrata"></canvas>
              </div>
              <div id="exits_container" class="<%= typeof showSelect == 'undefined' ? 'col-md-6' : '' %> col-12">
                <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                  <h1 class="h5">Grafico uscite <span class="total"></span></h1>
                </div>
                <canvas class="my-4 w-100" id="uscita"></canvas>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</main>